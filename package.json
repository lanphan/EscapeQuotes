{
	"name": "escape-quotes",
	"publisher": "milovidov",
	"displayName": "Escape-quotes",
	"description": "The extension allows you to escape quotes on the selected line.",
	"version": "1.0.5",
	"engines": {
		"vscode": "^1.30.0"
	},
	"categories": [
		"Other",
		"Formatters"
	],
	"icon": "images/icon.png",
	"activationEvents": [
		"onCommand:extension.screenQuotes",
		"onCommand:extension.screenUnquotes"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.screenQuotes",
				"title": "Escape quotes"
			},
			{
				"command": "extension.screenUnquotes",
				"title": "Unescape quotes"
			}
		],
		"menus": {
			"editor/context": [
				{
					"command": "extension.screenQuotes",
					"when": "editorHasSelection",
					"group": "7_modification"
				},
				{
					"command": "extension.screenUnquotes",
					"when": "editorHasSelection",
					"group": "7_modification"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.1.4",
		"vscode": "^1.1.25",
		"@types/node": "^8.10.25",
		"@types/mocha": "^2.2.42"
	}
}